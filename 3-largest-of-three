public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
        
    //Set a new integer
    Integer newnum;

    //Business logic for finding which is larger
    //Add '>=' to catch any of the same num scenarios
    if((num1 >= num2) && (num1 >= num3)){
        newnum = num1;
    } else if ((num2 > num1) && (num2 > num3)){
        newnum = num2;
    } else if ((num3 > num1) && (num3 > num2)){
        newnum = num3;
    }

    return newnum;

}
