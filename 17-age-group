public String ageGroup(Integer n) {
    //code here
    String ageGroupRet;

    if (n >= 0 && n <= 1) return 'Infant';
    else if (n >= 2 && n <= 14){
        ageGroupRet = 'Child';
    } else if (n >= 15 && n <= 21){
        ageGroupRet = 'Youth';
    } else ageGroupRet = 'Adult';

    return ageGroupRet;
}